			mesbox yabumiDir, Scale(350), Scale(45), 5, 259: mesy += Scale(45)
			yabumIId = stat
			ShowScrollBar objinfo_hwnd(yabumIId), SB_VERT, FALSE
		}
	
		if (optionMode == OPMODE_CAPTURE){
	
			objsize Scale(350), scale20

			saveNameSelStr = saveNameCapture
			saveNameSel = 0
