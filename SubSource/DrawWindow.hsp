			selectCapSaveDirIId = stat
			ShowScrollBar objinfo_hwnd(selectCapSaveDirIId), SB_VERT, FALSE
			
			pos Scale(14), mesy
			mes "外部ソフトの場所"
			pos Scale(200), mesy
