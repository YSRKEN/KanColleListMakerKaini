			mesbox listSaveDir, Scale(350), Scale(45), 5, 259 :mesy += Scale(45)
			listSaveDirIId = stat
			ShowScrollBar objinfo_hwnd(listSaveDirIId), SB_VERT, FALSE
			
			pos Scale(14), mesy
			mes "連続キャプチャ保存先"
			pos Scale(200), mesy
